{
    "name": "desdinova",
    "type": "module",
    "version": "0.1.2",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "astro check && astro build",
        "preview": "astro preview",
        "astro": "astro",
        "prettier": "npx prettier --write --list-different .",
        "release": "npm run astro check && npm run prettier && npx release-it",
        "changelog": "npx auto-changelog --hide-credit -t keepachangelog --sort-commits date-desc"
    },
    "auto-changelog": {
        "output": "CHANGELOG.md",
        "template": "keepachangelog",
        "sort-commits": "date-desc",
        "unreleased": true,
        "commitLimit": false
    },
    "dependencies": {
        "@astrojs/check": "^0.9.3",
        "@astrojs/mdx": "^3.1.4",
        "@astrojs/sitemap": "^3.1.6",
        "@astrojs/tailwind": "^5.1.0",
        "@astrolib/seo": "^1.0.0-beta.5",
        "@fontsource-variable/aleo": "^5.0.6",
        "@fontsource-variable/jetbrains-mono": "^5.0.22",
        "@fontsource-variable/montserrat": "^5.0.20",
        "@iconify-json/line-md": "^1.1.40",
        "@playform/compress": "^0.1.1",
        "astro": "^4.14.5",
        "astro-embed": "^0.7.2",
        "astro-icon": "^1.1.1",
        "flowbite": "^2.5.1",
        "tailwindcss": "^3.4.10",
        "typescript": "^5.5.4"
    },
    "devDependencies": {
        "@iconify-json/flowbite": "^1.1.6",
        "@iconify-json/logos": "^1.1.44",
        "@iconify-json/mdi": "^1.1.68",
        "@iconify-json/ph": "^1.1.14",
        "@iconify-json/tabler": "^1.1.120",
        "flowbite-typography": "^1.0.3",
        "prettier": "^3.2.5",
        "prettier-plugin-astro": "^0.14.0",
        "prettier-plugin-tailwindcss": "^0.6.6"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.9.5"
    }
}
